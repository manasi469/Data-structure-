#include<iostream>
using namespace std;
int main(){
int i,n;
int element;
cout<<"Enter size of array"<<endl;
cin>>n;
int a[n];
int low=0, upper=n-1;
int mid;
cout<<"enter array element"<<endl;
for(i=0;i<n;i++){
cin>>a[i];
}
cout<<"Enter searching element:"<<endl;
cin>>element;
while(low<=upper) {
mid=(low+upper)/2;
if(a[mid]==element){
cout<<"Element is found at position: "<<mid<<endl;

return 0;
break;
}
else if(a[mid]<element){
low=mid+1;
}
else {
upper=mid-1;
}
}
cout<<"Element is not found:"<<endl;
return 0;
}
